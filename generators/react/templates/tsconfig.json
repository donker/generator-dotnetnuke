{
    "compilerOptions": {
        "target": "es5",
        "module": "es6",
        "jsx": "react",
        "noImplicitAny": false,
        "sourceMap": true,
        "strictNullChecks": true,
        "allowJs": true,
        "outDir": "./dist"
    },
    "include": [
        "**/*.ts",
        "**/*.tsx"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}